generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Menu {
  id Int @id @default(autoincrement())
  title String
  description String?
  starter String?
  desert String?
  price Float
  imageUrl String?
  drinksIds Int[]
}

model Drink {
  id Int @id @default(autoincrement())
  title String
  imageUrl String?
}
